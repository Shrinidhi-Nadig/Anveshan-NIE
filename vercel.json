{
    "builds": [{
        "src": "ANVESHAN/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" },
        "commands":"pip install --target . --upgrade -r requirements.txt"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ANVESHAN/wsgi.py"
        }
    ]
}
